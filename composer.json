    "type": "library",
    "autoload": {
        "psr-4": {
            "ReneRoboter\\BerlinDistrict\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Ren√© Backhaus",
            "email": "rene.backhaus@axelspringer.com"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^7.4",
